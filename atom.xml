---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>Matt Rogers</title>
 <link href="http://mattr.info/atom.xml" rel="self"/>
 <link href="http://mattr.info/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://mattr.info/</id>
 <author>
     <name>Matt Rogers</name>
     <email>mattrogers@sbcglobal.net</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://mattr.info{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://mattr.info{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>
